{
  "name": "Change default reaction",
  "description": "",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "worker.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/image/logo.png",
      "32": "/image/logo.png",
      "48": "/image/logo.png",
      "128": "/image/logo.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://chat.zalo.me/*"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "/image/logo.png",
    "32": "/image/logo.png",
    "48": "/image/logo.png",
    "128": "/image/logo.png"
  }
}
